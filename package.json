{
  "name": "syloper-monolitic",
  "version": "1.0.0",
  "description": "Para este proyecto, el entorno de desarollo funciona como una aplicación monolítica.",
  "scripts": {
    "clean": "(rm -rf ./backend-syloper/node_modules & rm -rf ./front-syloper/node_modules & rm -rf ./front-syloper/.cache & rm -rf ./front-syloper/public)",
    "install":"npm i --prefix ./front-syloper",
    "start:f": "npm start --prefix ./front-syloper",
    "start:b": "npm run dev --prefix ./backend-syloper",
    "start:all": "npm run start:f & npm run start:b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devandmus/syloper-master.git"
  },
  "author": "A. Maldonado - J. López - J. Sánchez - Y. Bao",
  "bugs": {
    "url": "https://github.com/devandmus/syloper-master/issues"
  },
  "homepage": "https://github.com/devandmus/syloper-master#readme"
}
